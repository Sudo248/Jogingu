<resources>

    <style name="Widget.Theme.Jogingu.ActionBar.Fullscreen" parent="Widget.AppCompat.ActionBar">
        <item name="android:background">@color/black_overlay</item>
    </style>

    <style name="Widget.Theme.Jogingu.ButtonBar.Fullscreen" parent="">
        <item name="android:background">@color/black_overlay</item>
        <item name="android:buttonBarStyle">?android:attr/buttonBarStyle</item>
    </style>

    <style name="MyTimePickerDark" parent="@style/Theme.AppCompat.Light.Dialog">
        <item name="android:textColor">@color/main_color_normal</item>
        <item name="colorOnPrimary">@color/main_color_normal</item>
        <item name="colorAccent">@color/main_color_normal</item>
        <item name="colorControlNormal">@color/main_color_normal</item>
    </style>

    <style name="TextInputLayout" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="android:textColor">@color/black</item>
        <item name="boxStrokeColor">@color/main_color_normal</item>
        <item name="hintTextColor">@color/main_color_normal</item>
    </style>

</resources>